{
    "markdown": {
        "tags": ["foo", "bar"]
    },
    "plugins": ["node_modules/jsdoc-export-default-interop/dist/index"],
    "babel": {
        "extensions": ["js", "es6", "jsx"]
    },
    "source": {
        "include": ["src/"]
    },
    "opts": {
        "destination": "./doc/",
        "recurse": true
    }
}